;
; BIND data file for corp.net
;
$TTL    604800
@       IN      SOA     corp.net root.corp.net. (
                              2         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL

@       IN      NS      ns.corp.net.
ns      IN      A       192.168.56.53

external-app            IN      A       192.168.56.30
external-forward-proxy  IN      A       192.168.56.20